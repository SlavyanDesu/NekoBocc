    "scraper-api",
    "anime-scraper",
    "nekopoi-scraper"
  ],
  "author": "SlavyanDesu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IndonesianDev/NekoBocc/issues"
  },
  "homepage": "https://github.com/IndonesianDev/NekoBocc#readme",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
