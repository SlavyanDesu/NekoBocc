{
  "name": "nekobocc",
  "version": "1.2.0",
  "description": "A simple and lightweight Nekopoi scraper.",
  "main": "./dist/cjs/NekoBocc.js",
  "types": "./dist/types/NekoBocc.d.ts",
  "dependencies": {
    "axios": "^1.2.2",
    "cheerio": "^1.0.0-rc.5"
  },
  "type": "module",
  "scripts": {
    "test": "node test/test.js",
    "test:cjs": "node test/test.cjs",
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/NekoBocc.js",
      "require": "./dist/cjs/NekoBocc.js",
      "default": "./dist/esm/NekoBocc.js"
    }
  },
  "repository": {
