{
  "name": "nekobocc",
  "version": "1.3.3",
  "description": "A simple and lightweight Nekopoi scraper.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": ["dist"],
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "scraper",
    "nekopoi",
    "downloader",
    "api",
    "hentai"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --config .prettierrc --write \"**/*.ts\"",
    "test": "node ./test/test.js"
  },
  "author": "SlavyanDesu",
  "license": "MIT",
  "homepage": "https://github.com/IndonesianDev/NekoBocc#readme",
  "dependencies": {
    "axios": "^1.2.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IndonesianDev/NekoBocc.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}