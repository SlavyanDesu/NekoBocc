{
  "name": "nekobocc",
  "version": "1.5.0",
  "description": "A simple and lightweight NekoPoi scraper.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "scraper",
    "nekopoi",
    "downloader",
    "api",
    "hentai"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && tsconfig-to-dual-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run lint:fix && npm run build",
    "test": "ts-node src/index.ts"
  },
  "author": "SlavyanDesu",
  "license": "MIT",
  "homepage": "https://github.com/SlavyanDesu/NekoBocc#readme",
  "dependencies": {
    "axios": "^1.2.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlavyanDesu/NekoBocc.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cheerio": "^0.22.31",
    "eslint": "^8.32.0",
    "globals": "^16.0.0",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-to-dual-package": "^1.1.0",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.26.0"
  }
}
